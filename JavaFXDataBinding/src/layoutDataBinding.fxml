      <TextField fx:id="txtFieldFirstName" promptText="First Name">
         <StackPane.margin>
            <Insets left="20.0" right="20.0" top="40.0" />
         </StackPane.margin>
      </TextField>
      <TextField fx:id="txtFieldLastName" promptText="Last Name">
         <StackPane.margin>
            <Insets left="20.0" right="20.0" top="80.0" />
         </StackPane.margin>
      </TextField>
      <Text fx:id="txtShowInput" strokeType="OUTSIDE" strokeWidth="0.0">
         <StackPane.margin>
            <Insets top="130.0" />
         </StackPane.margin>
      </Text>
      <Button fx:id="btnDelete" mnemonicParsing="false" onAction="#deleteInputs" text="Delete Inputs">
         <StackPane.margin>
            <Insets top="180.0" />
         </StackPane.margin>
      </Button>
   </children>
</StackPane>
