<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<VBox alignment="TOP_CENTER" minHeight="300.0" minWidth="300.0" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
   <children>
      <TextField fx:id="txtFieldFirstName" promptText="First Name" />
      <TextField fx:id="txtFieldLastName" promptText="Last Name">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </TextField>
      <Text fx:id="txtShowInput" strokeType="OUTSIDE" strokeWidth="0.0">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Text>
      <Button fx:id="btnDelete" mnemonicParsing="false" onAction="#deleteInputs" text="Delete Inputs">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
